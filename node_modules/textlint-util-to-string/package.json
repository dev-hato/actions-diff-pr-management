  "version": "2.1.1",
  "description": "textlint util convert Paragraph Node to text with SourceMap.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
